<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
	<class lazy="false" name="com.floreantpos.model.RecepieItem" table="RECEPIE_ITEM">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		
		<property name="percentage" column="PERCENTAGE" type="java.lang.Double" />
		<property name="inventoryDeductable" column="INVENTORY_DEDUCTABLE" type="java.lang.Boolean" />
		
		<many-to-one class="com.floreantpos.model.InventoryItem" lazy="false" name="inventoryItem">
			<column name="INVENTORY_ITEM" />
		</many-to-one>
		
		<many-to-one class="com.floreantpos.model.Recepie" name="recepie" not-null="true">
			<column name="RECEPIE_ID" />
		</many-to-one>
	</class>
</hibernate-mapping>
